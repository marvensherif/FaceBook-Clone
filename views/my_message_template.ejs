<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <div id="messages">
    <% Object.keys(messages).forEach(function (type) { %>
      <% messages[type].forEach(function (message) { %>
        <%let x= "alert-"+type;%>
    <div class="alert alert-<%=type%>" >
  
       <%=message%>

    </div>

      <% }) %>
    <% }) %>
    </div>
  </body>
</html>
<script type="text/javascript">
  setTimeout(function() {
      $('#messages').fadeOut('fast');
  }, 2000);
</script>
